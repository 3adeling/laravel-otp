{
    "name": "erdemkeren/temporary-access",
    "description": "This package allows you to create temporary access grants for your users.",
    "homepage": "https://github.com/erdemkeren/temporary-access",
    "keywords": ["temporary", "access", "grant"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hilmi Erdem KEREN",
            "email": "erdemkeren@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.3",
        "illuminate/database": "^5.3",
        "nesbot/carbon": "~1.21"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Erdemkeren\\TemporaryAccess\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Erdemkeren\\TemporaryAccess\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
